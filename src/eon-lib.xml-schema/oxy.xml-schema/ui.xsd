<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
					 targetNamespace="urn:df:ox-2019:ui"
					 attributeFormDefault="unqualified"
					 elementFormDefault="qualified"
					 version="1.0"
					 xmlns:me="urn:df:ox-2019:ui"
					 xmlns:ox="urn:df:ox-2019:core"
					 xmlns:ox.dependencies="urn:df:ox-2019:component-model:dependencies">

	<xs:import namespace="urn:df:ox-2019:core"/>
	<xs:import namespace="urn:df:ox-2019:component-model:dependencies" />

	<xs:complexType name="UIDescription" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="ox.dependencies:DependencySupportDescriptionBase">
				<xs:sequence />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="UIXAppDescription" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="ox:CustomXAppDescriptionBase">
				<xs:sequence>
					<xs:element name="UIRef" type="ox:MetadataRef" nillable="true" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
</xs:schema>