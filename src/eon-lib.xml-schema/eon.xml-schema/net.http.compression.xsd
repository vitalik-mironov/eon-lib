	<xs:complexType name="CompressionHandlerDescription" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="ox.http:HandlerDescriptionBase">
				<xs:sequence>
					<xs:element name="Mode" type="me:ST_CompressionHandlerMode" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

</xs:schema>