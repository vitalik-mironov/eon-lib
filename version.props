<Project>

	<PropertyGroup>
		<IncludeSourceRevisionInInformationalVersion>true</IncludeSourceRevisionInInformationalVersion>
	</PropertyGroup>

	<PropertyGroup>
		<SourceRevisionId></SourceRevisionId>
		<!--//-->
		<AssemblyVersion>4.2.0</AssemblyVersion>
		<FileVersion>4.2.0</FileVersion>
		<PackageVersion>4.2.0</PackageVersion>
		<!--//-->
		<InformationalVersion>$(FileVersion)</InformationalVersion>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<PackageVersion>$(PackageVersion)-debug000</PackageVersion>
	</PropertyGroup>

</Project>